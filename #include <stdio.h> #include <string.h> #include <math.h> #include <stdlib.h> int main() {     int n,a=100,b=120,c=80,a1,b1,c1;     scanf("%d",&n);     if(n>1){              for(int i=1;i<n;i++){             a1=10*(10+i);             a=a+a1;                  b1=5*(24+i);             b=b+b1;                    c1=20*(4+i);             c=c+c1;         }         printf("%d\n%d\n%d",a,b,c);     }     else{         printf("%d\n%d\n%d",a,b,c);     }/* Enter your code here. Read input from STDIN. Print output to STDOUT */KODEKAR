#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int n,a=100,b=120,c=80,a1,b1,c1;
    scanf("%d",&n);
    if(n>1){     
        for(int i=1;i<n;i++){
            a1=10*(10+i);
            a=a+a1;     
            b1=5*(24+i);
            b=b+b1;       
            c1=20*(4+i);
            c=c+c1;
        }
        printf("%d\n%d\n%d",a,b,c);
    }
    else{
        printf("%d\n%d\n%d",a,b,c);
    }   
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
