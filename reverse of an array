#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int i, n;
    scanf("%d",&n);
    int a[n];
    for (i=0;i<n;i++)
    scanf("%d",&a[i]);
    for(i=n-1;i>=0;i--)
    {
    printf("%d",a[i]);
    printf(" "); 
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
  
